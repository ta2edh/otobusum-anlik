  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "start:dev": "expo start --dev-client --clear",
    "start:tunnel": "expo start --dev-client --tunnel",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:simulator": "expo run:ios --ios-simulator",
    "ios:build": "eas build --profile production --platform ios",
    "ios:build-dev": "eas build --profile development --platform ios",
    "ios:build-preview": "eas build --profile preview --platform ios",
    "ios:submit": "eas submit --platform ios",
    "ios:logs": "npx react-native log-ios",
    "lint": "eslint .",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "reset:cache": "npx expo start --clear",
    "build:production": "NODE_ENV=production eas build --profile production --platform ios",
    "submit:production": "eas submit --platform ios --profile production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@material/material-color-utilities": "^0.3.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/ionicons": "^7.4.0",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/query-async-storage-persister": "^5.62.0",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-persist-client": "^5.62.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "53.0.12",
    "expo-constants": "~17.1.2",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "^5.2.1",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-location": "~18.1.5",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.9",
    "expo-updates": "~0.28.15",
    "i18n-js": "^4.5.0",
    "ky": "^1.8.1",
    "react": "19.0.0",
    "react-native": "0.79.4",
    "react-native-clusterer": "^3.0.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "use-debounce": "^10.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^24.0.3",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "typescript": "^5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
